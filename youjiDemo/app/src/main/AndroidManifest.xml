<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="neet.com.youjidemo">

    <application
        android:name=".bean.UserDateApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.LoginActivity" />
        <activity android:name=".view.LogupActivity" />
        <activity android:name=".view.ShareActivity" />
        <activity android:name=".view.OthersActivity" />
        <activity android:name=".view.DetailActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.FoodActivity"></activity>
        <activity android:name=".view.PersonalCenterActivity"></activity>
        <activity android:name=".view.MyLoveActivity"></activity>
        <activity android:name=".view.IndexActivity"></activity>
    </application>

</manifest>